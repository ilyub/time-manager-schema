  "type": "object",
  "additionalProperties": false,
  "properties": {
    "method": {
      "const": "GET"
    },
    "path": {
      "const": "/"
    },
    "query": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "ver": {
          "type": "string"
        }
      },
      "required": [
